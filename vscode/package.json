{
  "name": "lofi",
  "description": "Lo-Fi - Diagram Editing in VS Code",
  "version": "0.0.1",
  "publisher": "Ruedi Steinmann",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruediste/lo-fi-mockups.git"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "lofi.editor",
        "displayName": "Lo-Fi Editor",
        "selector": [
          {
            "filenamePattern": "*.lofi.png"
          },
          {
            "filenamePattern": "*.lofi"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "foo": {
    "vscode:prepublish": "npm run package"
  },
  "scripts": {
    "lint": "eslint",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "22.x",
    "@types/vscode": "^1.100.0",
    "eslint": "^9.13.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vsce": "^2.15.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
