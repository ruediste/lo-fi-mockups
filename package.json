{
  "name": "lo-fi-mockups",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start-web": "VITE_VARIANT=web vite --open",
    "start-xwiki": "VITE_VARIANT=xwiki vite --open",
    "vscode-start-webview": "VITE_VARIANT=vscode vite --port=5177",
    "start-native": "npx @neutralinojs/neu run -- --window-enable-inspector /home/ruedi/Downloads/UIoutside.lofi",
    "start-native-webapp": "VITE_VARIANT=native vite",
    "build": "tsc -b && VITE_VARIANT=web vite build",
    "build-pages": "tsc -b && VITE_VARIANT=web vite build --base=/lo-fi-mockups/",
    "typecheck": "tsc --noEmit --watch --project tsconfig.app.json",
    "lint": "eslint .",
    "preview": "VITE_VARIANT=web vite build && VITE_VARIANT=web vite preview",
    "test": "jest --watch",
    "build-vscode": "VITE_VARIANT=vscode vite build --base=./ && rm -rf vscode/webview && mkdir -p vscode/webview && cp -r dist/* vscode/webview/",
    "build-webjar": "tsc -b && VITE_VARIANT=xwiki vite build --base=./ && rm -rf xwiki/src/main/webjar && mkdir -p xwiki/src/main/webjar && cp -r dist/* xwiki/src/main/webjar/",
    "native-build": "npx @neutralinojs/neu build --release",
    "native-build-webapp": "tsc -b && VITE_VARIANT=native vite build",
    "native-download": "npx @neutralinojs/neu update"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@neutralinojs/lib": "^6.5.0",
    "@popperjs/core": "^2.11.8",
    "@ruediste/html-to-image": "1.13.3",
    "@ruediste/png-transformer": "^0.0.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "classnames": "^2.5.1",
    "file-saver": "^2.0.5",
    "flatbush": "^4.4.0",
    "idb": "^8.0.2",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "rc-dock": "^3.3.0",
    "react": "^19.2.4",
    "react-bootstrap": "^2.10.9",
    "react-bootstrap-icons": "^1.11.5",
    "react-confirm": "^0.3.0",
    "react-dom": "^19.2.4",
    "react-dropzone": "^14.3.8",
    "react-observe-size": "^0.3.1",
    "react-router": "^7.4.1",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@neutralinojs/neu": "^11.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "sass": "^1.85.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-pwa": "^1.0.3"
  }
}
