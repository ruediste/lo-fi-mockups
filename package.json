    "@emotion/react": "^11.14.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "classnames": "^2.5.1",
    "file-saver": "^2.0.5",
    "flatbush": "^4.4.0",
    "html-to-image": "^1.11.13",
    "idb": "^8.0.2",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "rc-dock": "^3.3.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-bootstrap-icons": "^1.11.5",
    "react-confirm": "^0.3.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-observe-size": "^0.3.1",
