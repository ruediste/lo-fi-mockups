{
  "name": "lo-fi-mockups",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "build-pages": "tsc -b && vite build --base=/lo-fi-mockups/",
    "typecheck": "tsc --noEmit --watch --project tsconfig.app.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest --watch",
    "build-webjar": "tsc -b && vite build --base=./ && rm -rf xwiki/webjar/src/main/resources && mkdir -p xwiki/webjar/src/main/resources && cp -r dist/* xwiki/webjar/src/main/resources/"
  },
  "dependencies": {
